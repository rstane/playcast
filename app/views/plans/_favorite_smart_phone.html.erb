<% if favorite.present? %>
  <%= link_to( "", "javascript:void(0);", onClick: "toggle_favorite( '#{plan.id}', '#{klass}', 'delete', '#{form_authenticity_token}' )", class: "#{klass}cancell" ) %>
<% else %>
  <%= link_to( "", "javascript:void(0);", onClick: "toggle_favorite( '#{plan.id}', '#{klass}', 'create', '#{form_authenticity_token}' )", class: klass ) %>
<% end %>
